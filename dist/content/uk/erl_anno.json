		"modulesummary": "Abstract datatype for the annotations of the Erlang Compiler.",
		"description": {
			"p": [
				{
					"marker": {
						"id": "annotations"
					},
					"em": "annotations"
				},
				"The Erlang Token Scanner returns tokens with a subset of the following annotations, depending on the options:",
				"From this, the following annotation is derived:",
				"This module also supports the following annotations, which are used by various modules:",
				{
					"seealso": [
						{
							"marker": "erl_scan#column/1",
							"c": "column()"
						},
						{
							"marker": "erl_scan#end_location/1",
							"c": "end_location()"
						},
						{
							"marker": "erl_scan#line/1",
							"c": "line()"
						},
						{
							"marker": "erl_scan#location/1",
							"c": "location()"
						},
						{
							"marker": "erl_scan#text/1",
							"c": "text()"
						}
					],
					"c": "erl_scan"
				},
				{
					"seealso": [
						{
							"marker": "erl_parse#anno_from_term/1",
							"c": "anno_from_term()"
						},
						{
							"marker": "erl_parse#anno_to_term/1",
							"c": "anno_to_term()"
						},
						{
							"marker": "erl_parse#fold_anno/3",
							"c": "fold_anno()"
						},
						{
							"marker": "erl_parse#map_anno/2",
							"c": "map_anno()"
						},
						{
							"marker": "erl_parse#mapfold_anno/3",
							"c": "mapfold_anno()"
						},
						{
							"marker": "erl_parse#new_anno/1",
							"c": "new_anno()"
						}
					],
					"c": "erl_parse"
				}
			],
			"taglist": [
				{
					"tag": [
						{
							"c": "column"
						},
						{
							"c": "location"
						},
						{
							"c": "text"
						}
					],
					"item": [
						{
							"p": "The column where the token begins."
						},
						{
							"p": "The line and column where the token begins, or just the line if the column is unknown."
						},
						{
							"p": "The token's text."
						}
					]
				},
				{
					"tag": {
						"c": "line"
					},
					"item": {
						"p": "The line where the token begins."
					}
				},
				{
					"tag": [
						{
							"c": "file"
						},
						{
							"c": "generated"
						},
						{
							"c": "record"
						}
					],
					"item": [
						{
							"p": "A filename."
						},
						{
							"p": "A Boolean indicating if the abstract code is compiler-generated. The Erlang Compiler does not emit warnings for such code."
						},
						{
							"p": {
								"seealso": {
									"marker": "dialyzer:dialyzer",
