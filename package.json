{
  "name": "erldoc",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "homepage": "https://revenage.github.io/erldoc",
  "scripts": {
    "start": "parcel ./src/index.html --public-url '/erldoc'",
    "clear": "rm -rf ./dist",
    "prod": "rm -rf dist && parcel build ./src/index.html --public-url '/erldoc'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "push": "git add dist -f && git commit -m 'push -f dist' && git push origin --delete gh-pages && git subtree push --prefix dist origin gh-pages",
    "predeploy": "yarn prod",
    "deploy": "gh-pages -d dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "elm": "^0.19.0-no-deps",
    "elm-format": "^0.8.1",
    "gh-pages": "^2.0.1",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "elm-hot": "^1.1.0",
    "node-elm-compiler": "^5.0.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "sass": "^1.21.0"
  },
  "staticFiles": {
    "watcherGlob": false
  }
