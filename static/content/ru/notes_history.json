									]
								},
								{
									"p": [
										{
											"c": [
												"rd/2",
												"rf/0,1",
												"rl/0,1",
												"rp/1",
												"rr/1,2,3",
												"user_default.erl",
												"shell(3)"
											]
										},
										"*** POTENTIAL INCOMPATIBILITY ***",
										"Own Id: OTP-5063"
									]
								},
								{
									"p": [
										{
											"c": "string:to_integer/1,"
										},
										"Own Id: OTP-5081 Aux Id: OTP-5136"
									]
								},
								{
									"p": [
										{
											"c": [
												"dets:repair_continuation/2",
												"dets:select/1",
												"dets:select/3"
