		"section": {
			"title": "Limitations",
			"list": {
				"type": "bulleted",
				"item": [
					{
						"p": "Zip64 archives are not supported."
					},
					{
						"p": "Password-protected and encrypted archives are not supported."
					},
					{
						"p": "Only the DEFLATE (zlib-compression) and the STORE (uncompressed data) zip methods are supported."
					},
					{
						"p": "The archive size is limited to 2 GB (32 bits)."
					},
					{
						"p": "Comments for individual files are not supported when creating zip archives. The zip archive comment for the whole zip archive is supported."
					},
					{
						"p": "Changing a zip archive is not supported. To add or remove a file from an archive, the whole archive must be recreated."
					}
				]
			}
		},
